#include <cstdlib>
#include <iostream>
#include <string>

using namespace std;

class pralka
{
    private:
    string Nrpralki;
    int rokprodukcji;
    string Marka;
    string Model;

    public:

    void Ustawdane(string nrpralki,int rokprodukcji,string marka, string model)
    {
    this->Nrpralki()=Nrpralki;
    this->rokprodukcji()=rokprodukcji
    this->Marka()=Marka
    this->Model()=Model
    }
    void wypisznrpralki()
    {
        cout<< "nr pralki="<< Nrpralki() << endl;
    }
    void wypiszrokprodukcji()
    {
        cout<< "rok produkcji="<< rokprodukcji() << endl;
    }
    void wypiszmarka()
    {
        cout<< "Marka ="<< Marka() << endl;
    }
    void wypiszmodel()
    {
        cout<< "Model="<< Model() << endl;
    }


};
void pralka::wypisznrpralki()



